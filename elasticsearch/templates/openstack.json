{
  "template": "openstack-*",
  "mappings": {
    "_default_": {
      "_all": {
        "enabled": true
      },
      "properties": {
        "logdate": {
          "type": "date",
		  "format":"yyyy-MM-dd HH:mm:ss.SSS"||,
          "index": "not_analyzed"
        },
        "host": {
          "type": "string",
          "index": "not_analyzed"
        },
        "path": {
          "type": "string",
          "index": "not_analyzed"
        },
        "module": {
          "type": "string",
          "index": "not_analyzed"
        },
        "loglevel": {
          "type": "string",
          "index": "not_analyzed"
        },
        "request": {
          "type": "string",
          "index": "not_analyzed"
        },
        "referrer": {
          "type": "string",
          "index": "not_analyzed"
        },
        "received_at": {
          "type": "date",
          "index": "not_analyzed"
        },
        "requesterip": {
          "type": "string",
          "index": "not_analyzed"
        },
        "url_path": {
          "type": "string",
          "index": "not_analyzed"
        },
        "bytes": {
          "type": "long",
          "index": "not_analyzed"
        },
        "requestid": {
          "type": "string",
          "index": "not_analyzed"
        },
        "seconds": {
          "type": "float",
          "index": "not_analyzed"
        },
        "response": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    }
  }
}
