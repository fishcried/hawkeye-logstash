{
  "template": "openstack-*",
  "mappings": {
    "_default_": {
      "_all": {
        "enabled": false
      },
      "dynamic_templates": [
        {
          "string_template": {
            "match": "*",
            "mapping": {
              "type": "string",
              "index": "not_analyzed"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@fields": {
          "type": "object",
          "dynamic": true
        },
        "message": {
          "type": "string",
          "index": "not_analyzed"
        },
        "pid": {
          "type": "string",
          "index": "not_analyzed"
        },
        "type": {
          "type": "string",
          "index": "not_analyzed"
        },
        "logdate": {
          "type": "date",
		  "format":"yyyy-MM-dd HH:mm:ss.SSS",
          "index": "not_analyzed"
        },
        "@version": {
          "type": "string",
          "index": "not_analyzed"
        },
        "host": {
          "type": "string",
          "index": "not_analyzed"
        },
        "path": {
          "type": "string",
          "index": "not_analyzed"
        },
        "module": {
          "type": "string",
          "index": "not_analyzed"
        },
        "logmessage": {
          "type": "string",
          "index": "analyzed"
        },
        "tags": {
          "type": "string",
          "index": "not_analyzed"
        },
        "loglevel": {
          "type": "string",
          "index": "not_analyzed"
        },
        "request": {
          "type": "string",
          "index": "not_analyzed"
        },
        "referrer": {
          "type": "string",
          "index": "not_analyzed"
        },
        "@timestamp": {
          "type": "date",
          "index": "not_analyzed"
        },
        "received_at": {
          "type": "date",
          "index": "not_analyzed"
        },
        "requesterip": {
          "type": "string",
          "index": "not_analyzed"
        },
        "req_date": {
          "type": "string",
          "index": "not_analyzed"
        },
        "method": {
          "type": "string",
          "index": "not_analyzed"
        },
        "url_path": {
          "type": "string",
          "index": "not_analyzed"
        },
        "http_ver": {
          "type": "string",
          "index": "not_analyzed"
        },
        "bytes": {
          "type": "long",
          "index": "not_analyzed"
        },
        "requestid": {
          "type": "string",
          "index": "not_analyzed"
        },
        "seconds": {
          "type": "float",
          "index": "not_analyzed"
        },
        "response": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    }
  }
}
